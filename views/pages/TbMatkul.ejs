<div id="wrapperTbMatkul" class="pt-4 pb-4">
    <div class="container-fluid">
        <div class="card card-primary card-tabs">
            <div class="card-header p-0 pt-1">
                <ul class="nav nav-tabs" id="custom-tabs-tbmatkul-tab" role="tablist">
                    <li class="nav-item" style="cursor:pointer">
                        <span onclick="callList()" class="nav-link">Refresh List</span>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" id="custom-tabs-tbmatkul-list-tab" data-toggle="pill" href="#custom-tabs-tbmatkul-list" role="tab" aria-controls="custom-tabs-tbmatkul-list" aria-selected="true">LIST</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="custom-tabs-tbmatkul-form-tab" data-toggle="pill" href="#custom-tabs-tbmatkul-form" role="tab" aria-controls="custom-tabs-tbmatkul-form" aria-selected="false">FORM</a>
                    </li>
                </ul>
            </div>
            <div class="card-body p-0">
                <div class="tab-content" id="custom-tabs-tbmatkul-tabContent">
                    <div class="tab-pane fade show active" id="custom-tabs-tbmatkul-list" role="tabpanel" aria-labelledby="custom-tabs-tbmatkul-list-tab">
                        <div id="listData" class="card card-outline-primary">
                            <div class="card-header">
                                <h2><%= title_content %></h2>
                                <div class="row row-cols-4">
                                    <div class="col">
                                        <div class="form-group">
                                            <label for="fillter_nama_matkul">Filter Nama Mata Kuliah</label>
                                            <input onchange="callList(true)" type="text" class="form-control" id="fillter_nama_matkul" name="fillter_nama_matkul">
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="form-group">
                                            <label for="fillter_ket">Filter Keterangan</label>
                                            <input onchange="callList(true)" type="text" class="form-control" id="fillter_ket" name="fillter_ket">
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-cols-4">
                                    <div id="order_by" class="col">
                                        <label for="order_by">Order By</label>
                                        <select onchange="callList(true)" class="form-control" name="order_by" id="order_by">
                                            <option value="id_matkul">id_matkul</option>
                                            <option value="nama_matkul">nama_matkul</option>
                                            <option value="ket">ket</option>
                                        </select>
                                    </div>
                                    <div id="order_by_value" class="col">
                                        <label for="order_by_value">Order By Value</label>
                                        <select onchange="callList(true)" class="form-control" name="order_by_value" id="order_by_value">
                                            <option value="DESC">DESC</option>
                                            <option value="ASC">ASC</option>
                                        </select>
                                    </div>
                                    <div id="show" class="col">
                                        <div class="form-group">
                                            <label for="show">Tampil Data : <small><b id="start"></b> sampai <b id="end"></b></small></label>
                                            <select onchange="callList(true)" class="form-control" name="show" id="show">
                                                <option value="10">10</option>
                                                <option value="25">25</option>
                                                <option value="50">50</option>
                                                <option value="100">100</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div id="halaman" class="col">
                                        <div class="form-group">
                                            <label for="halaman">Halaman <small>(Batas akhir : <b></b>)</small></label>
                                            <input onchange="callList(false)" type="number" class="form-control" id="halaman" name="halaman" min="1" max="" value="1">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card-body p-0">
                                <div class="table-responsive">
                                    <table class="table table-striped table-sm">
                                        <thead>
                                            <tr>
                                                <th>tools</th>
                                                <th>id_matkul</th>
                                                <th>nama_matkul</th>
                                                <th>ket</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="custom-tabs-tbmatkul-form" role="tabpanel" aria-labelledby="custom-tabs-tbmatkul-form-tab">
                        <div id="formData" class="card">
                            <div class="card-header">Form <b></b></div>
                            <div class="card-body">
                                <form onsubmit="submitForm(); return false;">
                                    <div class="container-fluid">
                                        <div class="row row-cols-2 mb-3">
                                            <div class="col">
                                                <input name="id_matkul" type="hidden">
                                                <label for="nama_matkul" class="form-label">nama_matkul</label>
                                                <input name="nama_matkul" type="text" class="form-control" disabled>
                                            </div>
                                            <div class="col">
                                                <label for="ket" class="form-label">ket</label>
                                                <input name="ket" type="text" class="form-control" disabled>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="btn-group btn-block" role="group" aria-label="Basic example">
                                                <button type="submit" class="btn btn-sm btn-outline-success">Submit</button>
                                                <button onclick="clearForm()" type="reset" class="btn btn-sm btn-outline-danger">Cancel</button>
                                                <span onclick="addNew()" class="btn btn-sm btn-outline-info">Add Data</span>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>